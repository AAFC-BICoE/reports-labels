	{% if i == 0 %} -->
		<tr>
			<td class="main coll-1">
				{% block content %}
					cell 1
				{% endblock %}
			</td>
			<td class="coll-space">
				{% block collspace %}
					coll-space
				{% endblock %}
			</td>
	<!-- {% else %} -->
			<td class="main coll-2">
				{% block content %}
					cell 2
				{% endblock %}
			</td>
<!--	{% endif %} -->
	
		
 <!--   {% if i == cells-1 or p == cellsPerPage-1 or d == max-1 %} -->
		</tr>
		
	 <!--   {% if p < cellsPerPage-1 %} -->		
			<tr class="row-space">
				<td colspan="3">
				{% block rowspace %}
					row-space
				{% endblock %}
				</td>
			</tr>
	 <!--   {% endif %} -->	
		
		
